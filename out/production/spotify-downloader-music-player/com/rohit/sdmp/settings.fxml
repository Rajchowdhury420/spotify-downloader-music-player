<GridPane fx:id="settingsrootPane"  alignment="top_left" hgap="10" prefHeight="175.0" prefWidth="800.0" style="-fx-background-color: #262626;" vgap="10" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.rohit.sdmp.settingsController">
    <columnConstraints>
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
        <RowConstraints />
    </rowConstraints>
    <children>
        <Label prefHeight="49.0" prefWidth="800.0" text="Spotify Downloader Music Player" textFill="#ff5733">
            <font>
                <Font name="Comic Sans MS" size="24.0" />
            </font>
            <GridPane.margin>
                <Insets left="10.0" />
            </GridPane.margin></Label>
        <GridPane GridPane.rowIndex="1">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="392.0" minWidth="10.0" prefWidth="234.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="558.0" minWidth="10.0" prefWidth="558.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="551.0" minWidth="9.0" prefWidth="9.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <Button mnemonicParsing="false" onAction="#homePressed" prefHeight="30.0" prefWidth="275.0" style="-fx-background-color: black;" text="Home" textFill="#f5f5f5">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Button>
                <Button mnemonicParsing="false" onAction="#playlistPressed" prefHeight="45.0" prefWidth="257.0" style="-fx-background-color: black;" text="Playlist" textFill="WHITE" GridPane.rowIndex="1">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Button>
                <Button mnemonicParsing="false" onAction="#settingsPressed"  prefHeight="47.0" prefWidth="257.0" style="-fx-background-color: black;" text="Settings" textFill="WHITE" GridPane.rowIndex="2">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Button>
                <Button mnemonicParsing="false" onAction="#aboutPressed" prefHeight="51.0" prefWidth="257.0" style="-fx-background-color: black;" text="About" textFill="WHITE" GridPane.rowIndex="3">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Button>
            </children>
        </GridPane>
    </children>
</GridPane>
